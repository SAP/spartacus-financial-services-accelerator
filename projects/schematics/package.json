    "@angular/cdk": "^10.2.0",
    "@angular/localize": "^10.1.0",
    "@angular/pwa": "^0.1001.0",
    "@nguniversal/express-engine": "^10.1.0",
    "@schematics/angular": "^10.1.0",
    "@schematics/update": "^0.1001.0",
    "@spartacus/schematics": "3.0.0-next.3"
  },
  "devDependencies": {
    "@spartacus/schematics": "3.0.0-next.3",
    "@types/jasmine": "^3.5.0",
    "@types/node": "^12.11.1",
    "@types/shelljs": "^0.8.7",
    "jasmine": "^3.5.0",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.3",
    "typescript": "~4.0.2"

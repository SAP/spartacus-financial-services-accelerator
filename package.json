{
  "name": "fsastorefrontapp",
  "version": "1.0.0",
  "license": "Apache-2.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:qa": "ng build -c=qa",
    "build:ci": "ng build -c=ci",
    "build:prod": "ng build -c=production",
    "start:qa": "ng serve -c=qa",
    "start:ci": "ng serve -c=ci",
    "start:prod": "ng serve -c=prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "e2e:cy:open": "yarn --cwd ./projects/fsastorefront-e2e-cypress run cy:open",
    "e2e:cy:run": "yarn --cwd ./projects/fsastorefront-e2e-cypress run cy:run",
    "e2e:cy:start-open": "start-server-and-test start:ci http-get://localhost:4200 e2e:cy:open",
    "e2e:cy:start-run": "start-server-and-test start:ci http-get://localhost:4200 e2e:cy:run",
    "prettier": "prettier --config ./.prettierrc --list-different \"projects/**/*{.ts,.js,.json,.scss,.html}\"",
    "prettier-fix": "prettier --config ./.prettierrc --list-different \"projects/**/*{.ts,.js,.json,.scss,.html}\" --write",
    "generate:docs": "npx @compodoc/compodoc -p tsconfig.compodoc.json && . ./scripts/zip-docs.sh",
    "publish:docs": "gh-pages -d documentation -m \"Compodocs updates\"",
    "release:dynamicforms": "cd projects/dynamicforms && release-it && cd ../..",
    "release:fsastorefrontstyles": "cd projects/fsastorefrontstyles && release-it && cd ../..",
    "release:fsastorefront": "cd projects/fsastorefrontlib && release-it && cd ../..",
    "release:schematics": "cd projects/schematics && release-it && cd ../.."
  },
  "private": false,

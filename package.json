{
  "name": "fsastorefrontlib",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "cp -r ./projects/fsastorefrontstyles/fonts ./src/assets && ng build",
    "build:qa": "cp -r ./projects/fsastorefrontstyles/fonts ./src/assets && ng build -c=qa",
    "build:ci": "cp -r ./projects/fsastorefrontstyles/fonts ./src/assets && ng build -c=ci",
    "start:qa": "ng serve -c=qa",
    "start:ci": "ng serve -c=ci",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "e2e:cy:open": "yarn --cwd ./projects/fsastorefront-e2e-cypress run cy:open",
    "e2e:cy:run": "yarn --cwd ./projects/fsastorefront-e2e-cypress run cy:run",
    "e2e:cy:start-open": "start-server-and-test start:ci http-get://localhost:4200 e2e:cy:open",
    "e2e:cy:start-run": "start-server-and-test start:ci http-get://localhost:4200 e2e:cy:run",
    "prettier": "prettier --config ./.prettierrc --list-different \"projects/**/*{.ts,.js,.json,.scss,.html}\"",
    "prettier-fix": "prettier --config ./.prettierrc --list-different \"projects/**/*{.ts,.js,.json,.scss,.html}\" --write"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.0.0",
    "@angular/common": "~8.0.0",
    "@angular/compiler": "~8.0.0",
